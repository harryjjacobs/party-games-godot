[application]

config/name="party-games"
run/main_scene="res://memes/Memes.tscn"
config/icon="res://icon.png"

[autoload]

Events="*res://core/events/Events.gd"
NetworkInterface="*res://core/comms/NetworkInterface.tscn"
Room="*res://core/core_controllers/Room.gd"
BackgroundMusic="*res://core/audio/BackgroundMusic.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "meme_template_plugin" )

[network]

limits/debugger_stdout/max_chars_per_second=8192
limits/debugger_stdout/max_messages_per_frame=100
limits/debugger_stdout/max_errors_per_second=200
limits/debugger_stdout/max_warnings_per_second=200
limits/websocket_client/max_in_buffer_kb=2048
limits/websocket_client/max_in_packets=4096
limits/websocket_client/max_out_buffer_kb=2048
limits/websocket_client/max_out_packets=4096
limits/websocket_server/max_in_buffer_kb=2048
limits/websocket_server/max_in_packets=4096
limits/websocket_server/max_out_buffer_kb=2048
limits/websocket_server/max_out_packets=4096

[rendering]

environment/default_environment="res://default_env.tres"
